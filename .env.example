# Example environment variables - Copy this file and rename to .env, .env.development, or .env.production

# Node Environment (development, production, test)
NODE_ENV=development

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Application Configuration
REACT_APP_TITLE=All Ride User Management
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_MOCK_API=false
REACT_APP_ENABLE_DEBUG_MODE=false

# Note: All variables must be prefixed with REACT_APP_ to be accessible in React code 